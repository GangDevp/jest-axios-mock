{
  "name": "jest-axios-mock",
  "version": "1.0.1",
  "description": "A tool for test http request in jest.",
  "scripts": {
    "test": "jest test/index.test.js"
  },
  "jest": {
    "testRegex": ".*.test.js",
    "moduleFileExtensions": [
      "js"
    ],
    "moduleNameMapper": {
      "^jest-axios-mock(.*)$": "<rootDir>/src$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "author": "yinzhigang",
  "license": "ISC",
  "dependencies": {
    "jest": "^23.6.0",
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0"
  }
}
